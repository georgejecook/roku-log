{
  "cSpell.ignorePaths": [
    "**/package-lock.json",
    "**/node_modules/**",
    "**/vscode-extension/**",
    "**/.git/objects/**",
    ".vscode",
    "dist",
    "build",
  ],
  "files.exclude": {
    "build": true,
    ".history": true,
    "node_modules": true,
    "out": true,
    "**/.classpath": true,
    "**/.project": true,
    "**/.settings": true,
    "**/.factorypath": true,
    "/tmp/**": true,
    "**/dist": true,
    "**/build": true,
    "**/out": true
  },
  "search.exclude": {
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/node_modules/**": true,
    "**/.meteor/local": true,
    "**/build/**": true,
    "build/**": true,
    "**/.history/**": true,
    ".history/**": true,
    ".history/**/*": true,
    "out/**": true,
    "**/out/**": true,
    "/tmp/**": true,
    "**/dist": true,
    "**/build": true,
    "**/out": true
  },
  "files.useExperimentalFileWatcher": true,
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/node_modules/**": true,
    "**/.meteor/local": true,
    "**/build/**": true,
    "**/.history/**": true,
    ".history/**": true,
    "**/out/**": true,
    "**/dist": true,
    "**/build": true,
    "**/out": true
  },
  "cSpell.words": [
    "rlog"
  ],

}