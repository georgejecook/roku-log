<?xml version='1.0' encoding='UTF-8'?>
<component
    name='HTTPTransport'
    extends='Group'>

  <interface>
    <field
        id='maxLinesBeforeSending'
        type='integer'
        value='20' />
    <field
        id='maxSecondsBeforeSending'
        type='integer'
        value='5' />
    <field
        id='url'
        type='string'
        value='' />
    <field
        id='sendNow'
        type='boolean'
        value='false'
        alwaysNotify='true'
        onChange='sendLogsNow' />
    <field
        id='sendAutomatically'
        type='boolean'
        value='true' />
    <function name='logItem' />
    <function name='sendLogsNow' />
  </interface>

  <children>
  </children>
</component>
