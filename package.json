{
  "name": "roku-log",
  "version": "0.9.0",
  "description": "Simple logging library for roku.",
  "directories": {
    "doc": "docs"
  },
  "files": [
    "dist/**/!(manifest)*"
  ],
  "dependencies": {
    "bslib": "npm:@rokucommunity/bslib@^0.1.1"
  },
  "devDependencies": {
    "@types/chai": "^4.2.12",
    "@types/mocha": "^8.0.3",
    "@types/node": "^14.14.16",
    "@typescript-eslint/eslint-plugin": "^4.4.1",
    "@typescript-eslint/parser": "^4.4.1",
    "auto-changelog": "^2.2.1",
    "brighterscript": "^0.52.1",
    "chai": "^4.2.0",
    "debounce": "^1.2.0",
    "eslint": "^7.11.0",
    "eslint-plugin-github": "^4.1.1",
    "eslint-plugin-no-only-tests": "^2.4.0",
    "fs-extra": "^9.1.0",
    "lodash": "^4.17.20",
    "minimatch": "^3.0.4",
    "mocha": "^8.1.3",
    "nyc": "^15.1.0",
    "roku-deploy": "^3.2.4",
    "sax": "^1.2.4",
    "source-map": "^0.7.3",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3",
    "vscode-languageserver": "~6.1.1",
    "vscode-languageserver-protocol": "~3.15.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/georgejecook/roku-log.git"
  },
  "keywords": [
    "ropm",
    "brightscript",
    "mvvm",
    "framework",
    "fun"
  ],
  "author": "George Cook",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/georgejecook/roku-log/issues"
  },
  "homepage": "https://github.com/georgejecook/roku-log#readme",
  "ts-node": {
    "transpileOnly": true,
    "compileOptions": {
      "incremental": true,
      "allowJs": false
    }
  },
  "ropm": {
    "rootDir": "src",
    "packageRootDir": "dist",
    "prefixMatching": "expanded"
  },
  "scripts": {
    "build": "node scripts/run.js && ts-node scripts/move-generated-files.js",
    "build-test": "node scripts/run-test.js"
  }
}